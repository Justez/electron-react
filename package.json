{
  "name": "er-spa-app",
  "version": "1.0.0",
  "description": "Electron + React SPA playground",
  "main": "electron-main.js",
  "homepage": "app",
  "scripts": {
    "start": "cd app && yarn start",
    "install:app": "cd app && yarn install",
